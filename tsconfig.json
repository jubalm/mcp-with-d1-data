{
	"compilerOptions": {
		"target": "ES2021",
		"lib": [
			"ES2022",
			"DOM"
		],
		"jsx": "react-jsx",
		"module": "ES2022",
		"moduleResolution": "bundler",
		"allowImportingTsExtensions": true,
		"noEmit": true,
		"isolatedModules": true,
		"verbatimModuleSyntax": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true
	}
}
